<div id="bodyContainer" ng-app="SearchApp" ng-controller="searchController">
    <div class="container search-container">
        <div>
            <h1>Enter repo name:</h1>
        </div>
        <form>
            <div class="form-group">
                <input type="text" class="form-control" name="searchKey" id="searchKey" ng-model="searchKey" ng-keyup="SearchGithub(searchKey)" />
                <ul class="list-group" ng-hide="hideAutocomplete">
                    <li class="list-group-item" ng-repeat="repo in topRepoList track by $index" ng-click="FillTextBox(repo)">{{repo}}</li>
                </ul>
            </div>
        </form>
    </div>
</div>







